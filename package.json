{
  "name": "@sidy/eslint-config-sidy",
  "version": "1.4.11",
  "description": "A strict opinionated ESLint shareable config for the Sidy JavaScript style guide",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "node test/index.js",
    "lint": "eslint --no-ignore --no-eslintrc --plugin eslint-plugin-unicorn --config ./index.js --env node,es6 --parser-options=ecmaVersion:11 --fix .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/usidy/eslint-config-sidy.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "sidy"
  ],
  "license": "UNLICENSE",
  "publishConfig": {
    "access": "public"
  },
  "author": "sidy",
  "bugs": {
    "url": "https://github.com/usidy/eslint-config-sidy/issues"
  },
  "homepage": "https://github.com/usidy/eslint-config-sidy#readme",
  "dependencies": {
    "eslint": "^7.0.0",
    "eslint-plugin-unicorn": "^20.0.0"
  }
}
